{
 "name": "browsersync",
 "version": "1.0.0",
 "description": "https://github.com/Padke/zadanie_8_4.git",
 "main": "index.js",
 "scripts": {
  "test": "npm-run-all test:*",
  "test:html": "globstar nu-html-checker *.html",
  "test:js":"jshint js",
  "init-project": "npm install && npm-run-all init:*",
  "init:dirs": "mkdirp sass css vendor images js",
  "init:files": "touch README.md index.html sass/style.scss js/script.js",
  "init:gitignore": "curl https://raw.githubusercontent.com/github/gitignore/master/Node.gitignore -o .gitignore",
  "build": "npm-run-all build:* test",
  "build:sass": "node-sass --output-style compact -o css sass",
  "build:autoprefixer": "autoprefixer-cli css/style.css",
  "build-dev:sass": "node-sass --output-style expanded --source-map true -o css sass",
  "watch": "npm-run-all build -p watch:*",
  "watch:rimraf": "rimraf css/",
  "watch:sassprefixer": "OnChange sass/*.scss -- npm-run-all -p watch:sass watch:autoprefixer",
  "watch:sass": "node-sass --output-style expanded --source-map true -o css sass",
  "watch:autoprefixer": "autoprefixer-cli -o css/style.css css/style.css",
  "watch:browser-sync": "browser-sync"
 },
 "repository": {
  "type": "git",
  "url": "git+https://github.com/Padke/zadanie_8_3.git"
 },
 "keywords": [],
 "author": "",
 "license": "ISC",
 "bugs": {
  "url": "https://github.com/Padke/zadanie_8_3.git"
 },
 "homepage": "https://github.com/Padke/zadanie_8_3.git#readme",
 "devDependencies": {
  "autoprefixer-cli": "^1.0.0",
  "browser-sync": "^2.23.6",
  "globstar": "^1.0.0",
  "jshint": "^2.9.5",
  "mkdirp": "^0.5.1",
  "node-sass": "^4.7.2",
  "npm-run-all": "^4.1.2",
  "nu-html-checker": "^0.1.0",
  "onchange": "^3.3.0",
  "rimraf": "^2.6.2"
 },
 "dependencies": {}
}
